{
  "manifest_version": 3,
  "name": "WorkWell",
  "version": "1.0",
  "description": "Encourages quick workouts during your workday.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "alarms",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
}