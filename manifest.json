{
  "manifest_version": 3,
  "name": "WorkWell",
  "version": "1.0",
  "description": "Encourages quick workouts during your workday.",
  "permissions": [
    "storage",
    "alarms",
    "notifications",
    "identity"
  ],
  "host_permissions": [
    "https://www.googleapis.com/"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "oauth2": {
    "client_id": "528544209454-2fimspggacblf6absm9fj2tcigid7u2t.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  }
}